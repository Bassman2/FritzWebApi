<Application x:Class="FritzWebApiDemo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FritzWebApiDemo"
             xmlns:conv="clr-namespace:FritzWebApiDemo.Converter"
             Startup="OnApplicationStartup">
    <Application.Resources>
        <conv:ValueToStringConverter x:Key="ValueToStringConverter"/>
        <conv:DebugConverter x:Key="DebugConverter"/>
        <conv:BoolToBallImageConverter x:Key="BoolToBallImageConverter"/>
        <conv:EnumToStringConverter x:Key="EnumToStringConverter"/>
        <conv:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <conv:IntToFunctionBitMaskConverter x:Key="IntToFunctionBitMaskConverter"/>
        <conv:DeviceTypeToImageConverter x:Key="DeviceTypeToImageConverter"/>
    </Application.Resources>
</Application>
